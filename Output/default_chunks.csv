    p = q.dequeue()
    edgePix = [Pixel((p.x+1, p.y), pixelsL[p.x+1,p.y]),
               Pixel((p.x-1, p.y), pixelsL[p.x-1, p.y]),
               Pixel((p.x,
p.y+1), pixelsL[p.x, p.y+1]),
               Pixel((p.x, p.y-1), pixelsL[p.x, p.y-1])]
    for
pix in edgePix:
      if checkColors(pix.color, inside, 0.8):
        q.enqueue(pix)
       
pixelsL[pix.x, pix.y] = outside
  return img
