def Factorial(n):
    """""" Returns an int equal to n!
    Nonrecursive implementation
    PreC: n is a nonnegative int.
    """"""
    if n<=1:
        return 1
    else:
        z = 1
        for k in range(1,n+1):
            z = k*z
    return z
def FactorialR(n):
    """""" Returns an int equal to  n!
    Recursive implementation
    PreC: n is a nonnegative int.
    """"""
    if n<=1:
        return 1
  "
